CREATE SCHEMA [EASE_schema] [AUTHORIZATION admin]
[DEFAULT CHARACTER SET set_name]
CREATE TABLE PART()
CREATE TABLE AddApp (
  name TEXT NOT NULL,
  name TEXT NOT NULL,
  date TEXT NOT NULL,
  user TEXT NOT NULL,
  des TEXT NOT NULL,
  comp TEXT NOT NULL,
  price DOUBLE NOT NULL
  );
  
  CREATE TABLE Admin (
  app TEXT NOT NULL,
  apple TEXT NOT NULL,
  size TEXT NOT NULL,
  user TEXT NOT NULL,
  date TEXT NOT NULL
  );
  
  CREATE TABLE Application (
  name TEXT NOT NULL,
  userAdded TEXT NOT NULL,
  dateAdded TEXT NOT NULL,
  description TEXT NOT NULL,
  organization TEXT NOT NULL,
  link TEXT NOT NULL,
  category TEXT NOT NULL,
  isVerified INTEGER NOT NULL,
  price DOUBLE NOT NULL,
  rating DOUBLE NOT NULL
  );
  
  CREATE TABLE HomePage (
  app TEXT NOT NULL,
  apple TEXT NOT NULL,
  size TEXT NOT NULL,
  user TEXT NOT NULL,
  date TEXT NOT NULL
  );
  
  CREATE TABLE Login (
  userName TEXT NOT NULL,
  password TEXT NOT NULL
  );
  
  CREATE TABLE Login (
  firstName TEXT NOT NULL,
  lastName TEXT NOT NULL,
  userName TEXT NOT NULL UNIQUE,
  password TEXT NOT NULL
  );
  
  
  
GRANT ALL ON PART TO Customer
